CREATE TABLE servants 
    (id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT, 
    class TEXT,
    rarity INTEGER,
    attribute TEXT,
    alignment1 TEXT,
    alignment2 TEXT,
    np_type TEXT,
    np_card TEXT,
    debut TEXT,
    cv TEXT);

